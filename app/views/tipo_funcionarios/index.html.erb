<h1>Listing tipo_funcionarios</h1>

<table>
  <thead>
    <tr>
      <th>Tipo</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @tipo_funcionarios.each do |tipo_funcionario| %>
      <tr>
        <td><%= tipo_funcionario.tipo %></td>
        <td><%= link_to 'Show', tipo_funcionario %></td>
        <td><%= link_to 'Edit', edit_tipo_funcionario_path(tipo_funcionario) %></td>
        <td><%= link_to 'Destroy', tipo_funcionario, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tipo funcionario', new_tipo_funcionario_path %>
