<%= simple_form_for(@cliente) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :nome %>
    <%= f.input :endereco %>
    <%= f.input :foneResidencial %>
    <%= f.input :foneCelular %>
    <%= f.input :cpf %>
  </div>

  <div class="form-actions">
    <%= f.button :submit, "Cadastrar cliente"%>
  </div>
<% end %>
